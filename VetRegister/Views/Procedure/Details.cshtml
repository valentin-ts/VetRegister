@model ProcedureViewModel

@{
    ViewData["Title"] = "Procedure Details";
}

<form>
    <div class="form-group row">
        <label for="Description" class="col-sm-2 col-form-label">Description:</label>
        <a for="Description" class="col-sm-10 col-form-label">@Model.Description</a>
    </div>
    <div class="form-group row">
        <label for="CreatedOn" class="col-sm-2 col-form-label">CreatedOn:</label>
        <a for="CreatedOn" class="col-sm-10 col-form-label">@Model.CreatedOn</a>
    </div>
    <div class="form-group row">
        <label for="AnimalName" class="col-sm-2 col-form-label">AnimalName:</label>
        <a for="AnimalName" class="col-sm-10 col-form-label">@Model.AnimalName</a>
    </div>
    <div class="form-group row">
        <label for="DoctorName" class="col-sm-2 col-form-label">DoctorName:</label>
        <a for="DoctorName" class="col-sm-10 col-form-label">@Model.DoctorName</a>
    </div>
    <br>
    <a class="btn btn-primary" asp-controller="Procedure" asp-action="Edit" asp-route-id="@Model.Id">Edit procedure</a>
    <a class="btn btn-primary" asp-controller="Procedure" asp-action="Delete" asp-route-id="@Model.Id">Delete procedure</a>
</form>
