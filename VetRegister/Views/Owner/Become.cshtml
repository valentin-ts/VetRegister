@model BecomeOwnerFormModel

@{
    ViewData["Title"] = "Become Owner";
}

<form method="post">

    <div class="form-group">
        <label asp-for="Address"></label>
        <input asp-for="Address" class="form-control" placeholder="Owner Address">
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control" placeholder="Owner Phone Number">
    </div>
    <input class="btn btn-primary float-right" type="submit" value="Save" />
</form>